pipepline {
    agent any

    stages {
        stage('Checkout')
        {
            steps
            {
                sh 'echo "Checkout branch..."'
            }
        }
        stage('Build')
        {
            steps
            {
                sh 'echo "Running build..."'
            }
        }
        stage('Test')
        {
            steps
            {
                sh 'echo "Starting test..."'
            }
        }
        stage('Deploy')
        {
            steps
            {
                sh 'echo "Deploying..."'
            }
        }
    }
}